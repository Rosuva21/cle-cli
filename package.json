{
  "name": "@hyperoracle/zkgraph-cli",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.0",
  "description": "",
  "author": "MuRong <hi@mrong.me>",
  "license": "MIT",
  "homepage": "https://github.com/hyperoracle/zkgraph-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperoracle/zkgraph-cli.git"
  },
  "bugs": {
    "url": "https://github.com/hyperoracle/zkgraph-cli/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@murongg/eslint-config": "^0.2.0",
    "@types/node": "^18.18.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.3.0",
    "pnpm": "^8.8.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.31.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
